{
  "expo": {
    "name": "HC",
    "slug": "HC",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets2/images/logo.png",
    "scheme": "hc",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets2/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.muhmoud_505.HC"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets2/images/favicon.png"
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets2/images/img1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b9c85fcc-27eb-4828-bf3b-dcaf85a001f7"
      }
    }
  }
}
